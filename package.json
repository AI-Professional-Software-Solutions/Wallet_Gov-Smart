    "localforage": "^1.9.0",
    "node-fetch": "^3.0.0",
    "npm": "^7.24.2",
    "password-strength-calc": "^1.0.3",
    "qrcode-vue": "^1.2.0",
    "sanitize-html": "^2.4.0",
    "v-tooltip": "^2.1.2",
    "vue": "^2.6.12",
    "vue-async-computed": "^3.9.0",
    "vue-clickaway": "^2.2.2",
    "vue-clipboard2": "^0.3.1",
    "vue-qrcode-reader": "^2.3.18",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "~7.2",
    "@babel/preset-env": "~7.3",
    "@webpack-cli/serve": "^1.3.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "~10.0",
    "babel-loader": "~8.0",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^5.1.2",
    "cross-env": "~5.2",
    "css-loader": "~2.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^5.1.0",
